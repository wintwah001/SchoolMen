              <div class="card">
                <div class="card-header card-header-success">
                  <h4 class="card-title">Add Teacher</h4>
                  <p class="card-category"></p>

                </div>
                <div class="card-body">
                  <form action="/admin/teachers/teacheradd" method="POST" autocomplete="off">
                  <input type="hidden" name="_id" value="">
                    <div class="row">
                      <div class="col-md-6">
                        <div class="form-group">
                          <label class="bmd-label-floating">Name</label>
                          <input type="text" class="form-control" name="fullName" value="">
                          <div class="text-danger"></div>
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group">
                          <label class="bmd-label-floating">Gender  :</label>
                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="radio" value="Male" name="gender"  value="">Male
                        &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="radio" value="Female" name="gender"  value="">Female
                          <div class="text-danger"></div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-md-6">
                        <div class="form-group">
                          <label class="bmd-label-floating"></label>
                          <input type="date" class="form-control" name="DOB" value="" min="1900-01-01" max="2019-01-01">
                          <div class="text-danger"></div>
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group">
                          <label class="bmd-label-floating">NRC</label>
                          <input type="text" class="form-control" name="NRC" value="">
                          <div class="text-danger"></div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-md-4">
                        <div class="form-group">
                          <label class="bmd-label-floating">E-Mail</label>
                          <input type="email" class="form-control" name="email" value="" >
                          <div class="text-danger"></div>
                        </div>
                      </div>
                      <div class="col-md-4">
                        <div class="form-group">
                          <label class="bmd-label-floating">Role</label>
                          <input type="text" class="form-control" name="role"  value="">
                          <div class="text-danger"></div>
                        </div>
                      </div>
                      <div class="col-md-4">
                        <div class="form-group">
                          <label class="bmd-label-floating">Phone Number</label>
                          <input type="text" class="form-control" name="phNumber"  value="">
                          <div class="text-danger"></div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-md-6">
                        <div class="form-group">
                          <label class="bmd-label-floating">Password</label>
                          <input type="password" class="form-control" name="pwd"  value="">
                          <div class="text-danger"></div>
                        </div>
                      </div>
                      <div class="col-md-6">
                        <div class="form-group">
                          <label class="bmd-label-floating">Address</label>
                          <input type="text" class="form-control" name="address"  value="">
                          <div class="text-danger"></div>
                        </div>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-md-12">
                        <div class="form-group">
                          <label>About Teacher</label>
                          <div class="form-group">
                            <label class="bmd-label-floating">Description</label>
                            <textarea class="form-control" rows="3" name="description"  value=""></textarea>
                            <div class="text-danger"></div>
                          </div>
                        </div>
                      </div>
                    </div>

                    <button class="btn btn-danger pull-right hvr-bounce-to-right-cancel"><a href="/teacherDB/teacher"  style="color: white;">Cancel</a></button>
                    <button type="submit" class="btn btn-success pull-right hvr-bounce-to-right-submit">Submit</button>
                    <div class="clearfix"></div>
                  </form>
                </div>
              </div>
            </div>

          </div>
